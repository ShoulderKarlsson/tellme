    "dev-leap": "yarn build && node build/main.js --leap-year",
    "dev-red": "yarn build && node build/main.js -r"
  },
  "dependencies": {
    "@types/date-fns": "^2.6.0",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.2.4",
    "commander": "^2.15.1",
    "date-fns": "^1.29.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "backpack-core": "^0.7.0",
    "typescript": "^2.8.3"
  }
}
