{
  "name": "info-cli",
  "version": "1.0.0",
  "description": "simple help cli",
  "main": "index.js",
  "author": "shoulderkarlsson",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "start": "node build/main.js",
    "build": "backpack build",
    "dev-week": "yarn build && node build/main.js --week",
